form(
    (submit)='onRegisterSubmit()'
  )
  div
    label
      | Nick
    input(
        type='text',
        [(ngModel)]='nick',
        name='nick'
      )
  div
    label
      | Password
    input(
        type='password',
        [(ngModel)]='password',
        name='password'
      )
  input(
      type='submit',
      value='Submit'
    )
